{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"esnext",
			"dom",
			"dom.iterable"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"allowJs": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": false,
	}
}